<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Rate Your Experience"
        android:fontFamily="@font/poppins_semibold"
        android:textSize="19sp"
        android:textColor="@color/colorblack_five"
        android:gravity="center"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Help us improve!"
        android:fontFamily="@font/poppins_semibold"
        android:textColor="@color/colorwarm_grey_three"
        android:textSize="14sp"
        android:gravity="center"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_delivered"
        android:layout_gravity="center"/>
    <include
        android:id="@+id/view_ratingstar_layout"
        layout="@layout/view_ratingstar" />





</LinearLayout>